CREATE OR REPLACE VIEW CLV_CATALOG_DISTRIBUTORS ( CATALOG_ID,
DIST_BUS_ENT_ID ) AS
select catalog_id, bus_entity_id from clw_catalog_structure
where bus_entity_id is not null and catalog_structure_cd = 'CATALOG_PRODUCT'
group by catalog_id, bus_entity_id
;

CREATE OR REPLACE VIEW CLV_FACILITY_TYPE ( PROPERTY_ID,
BUS_ENTITY_ID, FACILITY_TYPE, SITE_NAME ) AS
Select prop.property_id, prop.bus_entity_id, prop.clw_value , bus.short_desc from CLW_PROPERTY prop, CLW_BUS_ENTITY bus
where bus.bus_entity_id = prop.bus_entity_id
and prop.short_desc = 'Facility Type:'
;

 CREATE OR REPLACE VIEW CLV_ITEM_INFO ( ITEM_ID,
SKU_NUM, ITEM_DESC, MANUFACTURER, MANU_SKU,
UOM, PACK, ITEM_SIZE, COLOR,
LIST_PRICE, CATEGORY ) AS
select CLW_ITEM.ITEM_ID, CLW_ITEM.SKU_NUM, CLW_ITEM.SHORT_DESC,
(select bus.short_desc from clw_bus_entity bus where bus.bus_entity_id=
(select map.bus_entity_id from clw_item_mapping map where map.item_id = clw_item.item_id
and map.item_mapping_cd = 'ITEM_MANUFACTURER')),
(select mp.item_num from clw_item_mapping mp where mp.item_id = clw_item.item_id and
mp.item_mapping_cd = 'ITEM_MANUFACTURER'),
(select uom_meta.clw_value from clw_item_meta uom_meta where uom_meta.name_value = 'UOM'
and clw_item.item_id = uom_meta.item_id (+)),
(select pack_meta.clw_value from clw_item_meta pack_meta where pack_meta.name_value = 'PACK'
and clw_item.item_id = pack_meta.item_id (+)),
(select size_meta.clw_value from clw_item_meta size_meta where size_meta.name_value = 'SIZE'
and clw_item.item_id = size_meta.item_id (+)),
(select color_meta.clw_value from clw_item_meta color_meta where color_meta.name_value = 'COLOR'
and clw_item.item_id = color_meta.item_id (+)),
(select list_price_meta.clw_value from clw_item_meta list_price_meta where list_price_meta.name_value = 'LIST_PRICE'
and clw_item.item_id = list_price_meta.item_id (+)),
(select min(it.short_desc)
from CLW_ITEM it,  CLW_ITEM_assoc ass
where  it.item_id=ass.item2_id and ass.ITEM_ASSOC_CD ='PRODUCT_PARENT_CATEGORY'
and ass.catalog_id = 4 and clw_item.ITEM_ID = ass.item1_id(+))
from CLW_ITEM
where clw_item.item_type_cd = 'PRODUCT'
order by CLW_ITEM.ITEM_ID asc
;

 CREATE OR REPLACE VIEW CLV_ITEM_USAGE ( ITEM_ID,
CONTRACT_ID, CONTRACT_NAME, CONTRACT_PRICE, CATALOG_ID,
CATALOG_NAME, DIST_ID, DIST_NAME, DIST_ERP_NUM,
DIST_SKU, DIST_COST, DIST_UOM, DIST_PACK,
DIST_SHORT_DESC ) AS
SELECT it.ITEM_ID, ci.CONTRACT_ID, c.SHORT_DESC, ci.AMOUNT,
c.CATALOG_ID, cat.SHORT_DESC, be.BUS_ENTITY_ID, be.SHORT_DESC,
be.ERP_NUM, im.ITEM_NUM, ci.DIST_COST, im.ITEM_UOM,
im.ITEM_PACK, im.SHORT_DESC
FROM CLW_ITEM it, CLW_CONTRACT_ITEM ci, CLW_CONTRACT c, CLW_CATALOG cat, CLW_CATALOG_STRUCTURE cs,
CLW_BUS_ENTITY be, CLW_ITEM_MAPPING im
WHERE it.ITEM_ID = ci.ITEM_ID and
ci.CONTRACT_ID = c.CONTRACT_ID and
c.CATALOG_ID = cat.CATALOG_ID and
((it.ITEM_ID = cs.ITEM_ID) AND (cat.CATALOG_ID = cs.CATALOG_ID)) and
cs.BUS_ENTITY_ID = be.BUS_ENTITY_ID and
((cs.BUS_ENTITY_ID = im.BUS_ENTITY_ID) AND (cs.ITEM_ID = im.ITEM_ID))
ORDER BY it.item_id, ci.CONTRACT_ID
;

 CREATE OR REPLACE VIEW CLV_TARGET_FACILITY ( PROPERTY_ID,
BUS_ENTITY_ID, SITE_NAME ) AS
Select prop.property_id, prop.bus_entity_id, bus.short_desc from CLW_PROPERTY prop, CLW_BUS_ENTITY bus
where bus.bus_entity_Id = prop.bus_entity_id
and prop.short_desc = 'Target Facility'
;

 CREATE OR REPLACE VIEW CLV_TOTAL_CONT_ITEMS_PURCH ( ITEM_ID,
CONTRACT_ID, LAST_DATE_ORDERED, TOTAL_QTY_ORDERED ) AS
select CLW_CONTRACT_ITEM.ITEM_ID, CLW_CONTRACT_ITEM.CONTRACT_ID, Max( ORIGINAL_ORDER_DATE) as L_DATE,
Sum( TOTAL_QUANTITY_ORDERED) as TOT_QTY from CLW_CONTRACT_ITEM, CLW_ORDER, CLW_ORDER_ITEM
where CLW_ORDER.ORDER_STATUS_CD <> 'Cancelled' and CLW_ORDER.ORDER_STATUS_CD <> 'Duplicate Order'
and CLW_ORDER.ORDER_STATUS_CD <> 'Duplicated' and CLW_ORDER.ORDER_STATUS_CD <> 'Rejected'
and CLW_ORDER.CONTRACT_ID = CLW_CONTRACT_ITEM.CONTRACT_ID and CLW_ORDER_ITEM.ORDER_ID = CLW_ORDER.ORDER_ID
and CLW_ORDER_ITEM.ITEM_ID = CLW_CONTRACT_ITEM.ITEM_ID group by CLW_CONTRACT_ITEM.ITEM_ID,
CLW_CONTRACT_ITEM.CONTRACT_ID
;

