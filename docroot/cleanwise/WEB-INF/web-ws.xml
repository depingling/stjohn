<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>SOAPWs</display-name>
    <servlet>
        <servlet-name>SOAPWs</servlet-name>
        <servlet-class>com.cleanwise.service.apps.ws.JBossSoapWeb</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>SOAPWs</servlet-name>
        <url-pattern>/SOAPWs</url-pattern>
    </servlet-mapping>
    <session-config>
      <session-timeout>${wsSessionTimeout}</session-timeout>
    </session-config>
</web-app>